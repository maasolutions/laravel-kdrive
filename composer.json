{
    "name": "maa-solutions/laravel-kdrive",
    "description": "Infomaniak kDrive Storage driver for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Marc-Andr√© Appel",
            "email": "contact@maa.rocks"
        },
        {
            "name": "Nicolas Hedger",
